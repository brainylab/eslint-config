{
	"name": "@brainylab/eslint-config-react",
	"version": "3.2.0",
	"description": "ESLint react config",
	"keywords": [
		"brainylab",
		"eslint",
		"config",
		"react"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/brainylab/eslint-config"
	},
	"license": "MIT",
