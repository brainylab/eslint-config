{
	"name": "@brainylab/eslint-config-typescript",
	"version": "3.1.4",
	"description": "ESLint typescript config",
	"keywords": [
		"brainylab",
		"eslint",
		"config",
		"typescript"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/brainylab/eslint-config"
	},
	"license": "MIT",
	"author": "Andr√© Felipe Schulle <andrefelipeschulle@icloud.com>",
	"type": "module",
	"main": "./src/index.ts",
	"files": [
		"dist",
		"LICENSE"
	],
	"scripts": {
		"build": "rimraf dist && tsup --dts",
		"clean": "rimraf dist .turbo",
		"prepack": "clean-package",
		"postpack": "clean-package restore",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@brainylab/eslint-config-base": "workspace:*",
		"eslint": "9.15.0",
		"eslint-define-config": "2.1.0",
		"globals": "15.12.0",
		"typescript-eslint": "8.16.0"
	},
	"devDependencies": {
		"clean-package": "2.2.0"
	},
	"peerDependencies": {
		"eslint": ">=9",
		"typescript": ">=5"
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"clean-package": "../clean-package.config.json"
}
