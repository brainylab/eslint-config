{
	"name": "@brainylab/eslint-config-base",
	"version": "3.1.5",
	"description": "ESLint base config",
	"keywords": [
		"brainylab",
		"eslint",
		"config",
		"base"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/brainylab/eslint-config"
	},
	"license": "MIT",
